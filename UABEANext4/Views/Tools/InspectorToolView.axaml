<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:um="using:UABEANext4.Controls"
             xmlns:tools="using:UABEANext4.ViewModels.Tools"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="UABEANext4.Views.Tools.InspectorToolView"
             x:DataType="tools:InspectorToolViewModel">
  <Grid Background="#252526">
    <Border Background="#1e1e1e" CornerRadius="8" Margin="8">
      <um:AssetDataTreeView
        Workspace="{Binding Workspace}"
        ActiveAssets="{Binding ActiveAssets}"
        Margin="4" />
    </Border>
  </Grid>
</UserControl>
